// C program to find the factorial number
#include<stdio.h>
int main()
{
	int i, fact, n;
	printf("Enter your number fo find it's factorial: ");
	fflush(stdin); fflush(stdout);
	scanf("%d", &n);

	for(i = 1; i <= n; i++)
	{
		fact *= i;
	}
	printf("The factoral number of (%d) = %d", n, fact);
	return 0;
}
