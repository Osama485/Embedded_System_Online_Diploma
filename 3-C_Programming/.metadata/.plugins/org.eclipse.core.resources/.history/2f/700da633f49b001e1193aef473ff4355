// Write Source Code to Swap Two Numbers without temp variable.

#include<stdio.h>
int main()
{
	int n1 = 50, n2 = 100;
	printf("Befor swap \nn1 = %d   &&&&&   n2 = %d", n1, n2);
	n1 = n1 + n2; // n1 50+100    = 150
	n2 = n1 - n2; // n2 = 150-100 = 50
	n1 = n1 - n2; // n1 = 150-50  = 100

	printf("After swap \nn1 = %d   &&&&&   n2 = %d", n1, n2);
	return 0;
}
