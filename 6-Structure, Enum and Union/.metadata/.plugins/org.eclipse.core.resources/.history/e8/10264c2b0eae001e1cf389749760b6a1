#include <stdio.h>
struct student
{
	short roll;
	char name[20];
	short marks;
};

struct student enter()
{
	struct student st;
	int i;
	for(i=1; i<=10; i++)
	{

		printf("for roll number ");
		fflush(stdout);
		scanf("%d", &st.roll[i+1]);

		printf("Enter name ");
		fflush(stdout);
		scanf("%s", &st.name[i+1]);

		printf("Enter marks ");
		fflush(stdout);
		scanf("%d", &st.marks[i+1]);
	}
	return st;
}
int main()
{

	return 0;
}
