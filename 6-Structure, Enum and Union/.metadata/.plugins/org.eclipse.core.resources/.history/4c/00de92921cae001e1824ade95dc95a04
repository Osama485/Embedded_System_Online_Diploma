#include <stdio.h>
struct student
{
	int roll;
	char name[20];
	int marks;
};



void printRes()
{
	struct student st;
	int i;
	for(i=1; i<=10; i++)
	{
		printf("information for roll number: %d \nName: %s \nMarks: %d", st.roll, st.name, st.marks);
	}
}
int main()
{
	struct student st[10];printf("Enter name ");
	fflush(stdout);
	scanf("%s", &(st[i].name));
	int i;
	for(i = 0; i < 10; i++)
	{
		printf("for roll number:  %d\n", i+1);

		printf("Enter name ");
		fflush(stdout);
		scanf("%s", &(st[i].name));

		printf("Enter marks ");
		fflush(stdout);
		scanf("%d", &(st[i].marks));
	}
	return 0;
}
