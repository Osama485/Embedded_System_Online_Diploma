#include<stdio.h>

struct Distance
{
	double feet;
	double inch;
};

struct Distance ReadDis()
{
	struct Distance d;
	printf("Enter information for 1st distance \nEnter real and imaginary: ");
	fflush(stdout);
	scanf("%lf %lf", &d.feet, &d.inch);
	return d;
}

int main()
{

}
