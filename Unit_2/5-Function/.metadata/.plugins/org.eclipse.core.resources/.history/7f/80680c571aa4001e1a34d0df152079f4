#include <stdio.h>
#include <string.h>
int main()
{
	int n1[100], n2 [100];

	printf("Enter your name: ");
	fflush(stdin); fflush(stdout);
	scanf("%s", &n1);
	printf("Enter string: ");
	fflush(stdin); fflush(stdout);
	scanf("%s", &n2);

	if(stricmp(n1) == stricmp(n2))
		printf("congratulation....\n");
	else
		printf("Wrong...\n");
	return 0;
}
