#include <stdio.h>
#include <string.h>
int main()
{
	int n1[100], n2 [100];

	printf("Enter your name: ");
	fflush(stdin); fflush(stdout);
	gets(n1);
	printf("Enter string: ");
	gets(n2);

	if(strcmp(n1) == strcmp(n2))
		printf("congratulation....\n");
	else
		printf("Wrong...\n");
	return 0;
}
