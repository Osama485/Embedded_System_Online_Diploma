#include <stdio.h>
#include <string.h>
int main()
{
	int n1[100], n2 [100];

	printf("Enter your name: ");
	gets(n1);
	printf("Enter string: ");
	gets(n2);
	if(stricmp(n1, n2) == 1)
		printf("congratulation....\n");
	else
		printf("Wrong...\n");
	return 0;
}
