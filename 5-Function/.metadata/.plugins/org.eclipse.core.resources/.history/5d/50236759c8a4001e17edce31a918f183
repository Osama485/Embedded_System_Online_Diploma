#include<stdio.h>
#include<string.h>
int main()
{
	char name[] = "osama";
	char string[100];
	printf("Enter your name: ");
	gets(string);

	if(stricmp(name, string) == 1)
		printf("true");
	else
		printf("Wrong");

	return 0;
}
