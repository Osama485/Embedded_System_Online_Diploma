#include <stdio.h>
#include <string.h>
int main()
{
	int n1[5], n2 [5], temp[5];
	int i;

	printf("Enter the first Array: \n");
	fflush(stdin);
	for(i = 0; i < 10; i++)
	{
		scanf("%d ", i);
	}

	printf("Enter the first Array: \n");
	fflush(stdin);
	for(i = 0; i < 10; i++)
	{
		scanf("%d ", i);
	}
	for(i = 0; i < 10; i++)
	{
		temp[i] = n1[i];
		n1[i] = n2[i];
		n2[i] = temp[i];
	}
	for(i = 0; i < 10; i++)
	{
		printf("%d\t", n1[i]);
	}
	for(i = 0; i < 10; i++)
	{
		printf("%d\t", n2[i]);
	}
	return 0;
}
